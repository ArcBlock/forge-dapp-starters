{
  "name": "forge-keystone-starter",
  "license": "Apache-2.0",
  "keywords": [
    "arcblock",
    "forge",
    "starter",
    "keystone",
    "javascript"
  ],
  "scripts": {
    "postinstall": "yarn patch",
    "patch": "node tools/patch.js",
    "lint": "eslint api src app.js",
    "build": "next build src",
    "clean": "rm -rf src/dist api/dist",
    "export": "next build src && next export src -o src/dist",
    "lambda:netlify": "netlify-lambda build api/functions",
    "lambda:serve": "netlify-lambda serve api/functions",
    "netlify": "npm run clean && npm run export && npm run lambda:netlify",
    "start": "nodemon app.js -w api -w app.js"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx,css,less,scss,json,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@arcblock/did": "^1.0.39",
    "@arcblock/did-auth": "^1.1.3",
    "@arcblock/did-auth-storage-mongo": "^1.0.13",
    "@arcblock/did-react": "^0.72.0",
    "@arcblock/forge-message": "^1.1.1",
    "@arcblock/forge-sdk": "^1.1.1",
    "@arcblock/forge-util": "^1.0.33",
    "@arcblock/forge-wallet": "^1.0.39",
    "@arcblock/mcrypto": "^1.0.39",
    "@arcblock/react-hooks": "^0.72.0",
    "@arcblock/ux": "^0.72.0",
    "@babel/polyfill": "^7.4.4",
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "4.5.1",
    "@material-ui/styles": "^4.3.3",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "core-js": "2.5.7",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-bearer-token": "^2.4.0",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "keystone": "^4.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "5.4.23",
    "morgan": "^1.9.1",
    "multibase": "^0.6.0",
    "netlify-lambda": "^1.4.7",
    "next": "^9.0.5",
    "next-compose-plugins": "^2.2.0",
    "next-plugin-custom-babel-config": "^1.0.2",
    "next-transpile-modules": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-use": "^13.26.3",
    "serverless-http": "^2.0.1",
    "store": "^2.0.12",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.10.0",
    "chai": "^4.2.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "set-npm-auth-token-for-ci": "^2.0.13",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  }
}
